  },
  "engines": {
    "node": ">=6"
  },
  "editions": [
    {
      "description": "typescript source code with import for modules",
      "directory": "source",
      "entry": "index.ts",
      "engines": false,
      "tags": [
        "typescript",
        "import"
      ]
    },
    {
      "description": "typescript compiled for node.js >=11 with require for modules",
      "directory": "edition-node-11",
      "entry": "index.js",
      "engines": {
        "node": ">=11",
        "browsers": false
      },
      "tags": [
        "javascript",
        "require"
      ]
    },
    {
      "description": "typescript compiled for node.js >=10 with require for modules",
      "directory": "edition-node-10",
      "entry": "index.js",
      "engines": {
        "node": ">=10",
        "browsers": false
      },
      "tags": [
        "javascript",
        "require"
      ]
    },
    {
      "description": "typescript compiled for node.js >=6 with require for modules",
      "directory": "edition-node-6",
      "entry": "index.js",
      "engines": {
        "node": ">=6",
        "browsers": false
      },
      "tags": [
        "javascript",
        "require"
      ]
    }
  ],
  "types": "source/index.ts",
  "main": "index.js",
  "dependencies": {
    "editions": "^2.1.0",
    "native-promise-pool": "^1.0.0",
    "taskgroup": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "@types/node": "^10.12.9",
    "assert-helpers": "^4.5.1",
    "eslint": "^5.9.0",
    "joe": "^2.0.2",
    "joe-reporter-console": "^2.0.2",
    "projectz": "^1.5.3",
    "surge": "^0.20.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6",
    "typescript-eslint-parser": "^21.0.1",
    "valid-directory": "^1.0.0"
  },
  "scripts": {
    "our:clean": "rm -Rf ./docs ./edition* ./es2015 ./es5 ./out",
    "our:compile": "npm run our:compile:edition-node-10 && npm run our:compile:edition-node-11 && npm run our:compile:edition-node-6",
    "our:compile:edition-node-10": "env BABEL_ENV=edition-node-10 babel --extensions \".ts,.tsx\" --out-dir ./edition-node-10 ./source",
    "our:compile:edition-node-11": "env BABEL_ENV=edition-node-11 babel --extensions \".ts,.tsx\" --out-dir ./edition-node-11 ./source",
    "our:compile:edition-node-6": "env BABEL_ENV=edition-node-6 babel --extensions \".ts,.tsx\" --out-dir ./edition-node-6 ./source",
    "our:deploy": "echo no need for this project",
    "our:meta": "npm run our:meta:docs && npm run our:meta:projectz",
    "our:meta:docs": "typedoc --name \"$npm_package_name\" --readme ./README.md --out ./docs ./source",
    "our:meta:projectz": "projectz compile",
    "our:release": "npm run our:release:prepare && npm run our:release:check-changelog && npm run our:release:check-dirty && npm run our:release:tag && npm run our:release:push",
    "our:release:check-changelog": "cat ./HISTORY.md | grep v$npm_package_version || (echo add a changelog entry for v$npm_package_version && exit -1)",
    "our:release:check-dirty": "git diff --exit-code",
    "our:release:prepare": "npm run our:clean && npm run our:compile && npm run our:test && npm run our:meta",
    "our:release:push": "git push origin master && git push origin --tags",
    "our:release:tag": "export MESSAGE=$(cat ./HISTORY.md | sed -n \"/## v$npm_package_version/,/##/p\" | sed 's/## //' | awk 'NR>1{print buf}{buf = $0}') && test \"$MESSAGE\" || (echo 'proper changelog entry not found' && exit -1) && git tag v$npm_package_version -am \"$MESSAGE\"",
    "our:setup": "npm run our:setup:npm",
    "our:setup:npm": "npm install",
    "our:test": "npm run our:verify && npm test",
    "our:verify": "npm run our:verify:directory && npm run our:verify:eslint && npm run our:verify:typescript",
    "our:verify:directory": "npx valid-directory",
    "our:verify:eslint": "eslint --fix ./source/**",
    "our:verify:typescript": "tsc --project .",
    "test": "node --harmony ./test.js --joe-reporter=console"
  },
  "babel": {
    "env": {
      "edition-node-11": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "11"
              }
            }
          ],
          "@babel/preset-typescript"
        ],
        "plugins": [
          "@babel/proposal-class-properties",
          "@babel/proposal-object-rest-spread"
        ]
      },
      "edition-node-10": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "10"
              }
            }
          ],
          "@babel/preset-typescript"
        ],
        "plugins": [
          "@babel/proposal-class-properties",
          "@babel/proposal-object-rest-spread"
        ]
      },
      "edition-node-6": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "6"
              }
            }
          ],
          "@babel/preset-typescript"
        ],
        "plugins": [
          "@babel/proposal-class-properties",
          "@babel/proposal-object-rest-spread"
        ]
      }
    }
  },
  "optionalDependencies": {}
}
